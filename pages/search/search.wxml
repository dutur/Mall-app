<view class="search_row">
  <input placeholder="请输入您要搜索的商品" bindinput="handleInput" value="{{inpValue}}"></input>
  <button size="mini" hidden="{{!isFocus}}" bindtap="handleCancel">取消</button>
</view>
<view class="search_content">
  <navigator class="search_item" wx:for="{{goods}}" wx:key="goods_id" url="/pages/goods_detail/detail?goods_id={{item.goods_id}}">
    {{item.goods_name}}
  </navigator>
</view>
